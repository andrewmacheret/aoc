{
  "runOnSave.statusMessageTimeout": 3000,
  "runOnSave.commands": [
    {
      "match": ".*\\.py$",
      "command": "python.execInTerminal",
      "runIn": "vscode"
    },
    {
      "match": ".*/util\\.py$",
      "command": "${fileDirname}/../setup; ${fileDirname}/../setup",
      "runIn": "terminal"
    }
  ],
  "python.formatting.provider": "autopep8"
}
